{
  "framework": "astro",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "${PUBLIC_API_URL}/api/:path*"
    }
  ],
  "env": {
    "PUBLIC_API_URL": ""
  }
}
